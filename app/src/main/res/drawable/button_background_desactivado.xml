<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <size android:width="100dp" android:height="35dp" />
            <corners android:radius="15dp" />
            <stroke android:width="2px" android:color="@color/colorIconosOpaco" />
            <gradient android:angle="270" android:centerColor="@color/colorIconosOpaco" android:endColor="@color/colorTextoTitulos" android:startColor="@color/colorIconosOpaco" />
        </shape>
    </item>
</selector>