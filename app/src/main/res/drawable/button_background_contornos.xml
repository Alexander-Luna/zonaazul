<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="50dp" />
            <size android:width="50dp" android:height="50dp" />
            <stroke android:width="2dp" android:color="@color/colorprin" />
            <gradient android:angle="270" android:endColor="@color/colorFondo" android:startColor="@color/colorprin" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="50dp" />
            <size android:width="50dp" android:height="50dp" />
            <stroke android:width="1dp" android:color="@color/colorprin" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</selector>